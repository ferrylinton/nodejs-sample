{
	"name": "mongodb-crud",
	"version": "1.0.0",
	"description": "Node JS CRUD Application With Mongodb",
	"main": " src/index.js",
	"scripts": {
		"start": "cross-env NODE_ENV=production node src/index.js",
		"dev": "cross-env NODE_ENV=development node src/index.js",
		"test": "cross-env NODE_ENV=test jest --runInBand --detectOpenHandles --forceExit",
		"lint": "eslint .",
		"lint:fix": "eslint --fix .",
		"format": "prettier . --write",
		"format:check": "prettier . --check",
		"doc": "rimraf docs && jsdoc -c jsdoc.json"
	},
	"keywords": [
		"node",
		"mongodb",
		"eslint",
		"jest"
	],
	"author": "ferrylinton",
	"license": "ISC",
	"devDependencies": {
		"docdash": "^2.0.2",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jest": "^27.6.0",
		"eslint-plugin-prettier": "^5.0.1",
		"jest": "^29.7.0",
		"jsdoc": "^4.0.2",
		"mongodb-memory-server": "^7.6.3",
		"prettier": "^3.1.1",
		"rimraf": "^5.0.5"
	},
	"dependencies": {
		"cross-env": "^7.0.3",
		"dotenv": "^16.3.1",
		"mongodb": "^6.3.0"
	}
}
