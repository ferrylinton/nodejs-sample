{
	"source": {
		"include": "src",
		"includePattern": ".js$"
	},
	"opts": {
		"template": "node_modules/docdash",
		"destination": "./docs/",
		"readme": "./readme.md",
		"recurse": true
	},
	"docdash": {
		"sort": true,
		"search": true,
		"menu": {
			"Github repo": {
				"href": "https://github.com/ferrylinton/nodejs-sample/tree/main/jsdoc-crud",
				"target": "_blank",
				"class": "menu-item",
				"id": "repository"
			}
		}
	}
}
